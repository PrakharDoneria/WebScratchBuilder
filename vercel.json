{
  "version": 2,
  "buildCommand": "./vercel-build.sh",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "functions": {
    "api/projects.ts": {
      "runtime": "nodejs18.x",
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/api/projects", "dest": "/api/projects.ts" },
    { "src": "/api/projects/(.*)", "dest": "/api/projects.ts?id=$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}